{% extends 'base.html.twig' %}

{% block title %}Assure{% endblock %}

{% block body %}
    <h1>Assure</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ assure.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ assure.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ assure.prenom }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ assure.tel }}</td>
            </tr>
            <tr>
                <th>DateNaissance</th>
                <td>{{ assure.dateNaissance ? assure.dateNaissance|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_assure_index') }}">back to list</a>

    <a href="{{ path('app_assure_edit', {'id': assure.id}) }}">edit</a>

    {{ include('assure/_delete_form.html.twig') }}
{% endblock %}
